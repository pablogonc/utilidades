#include <commons/collections/dictionary.h>
#include <commons/config.h>
#include <readline/readline.h>
int main(){

	t_config* confi = config_create("nugaton.txt");
	char* leido;
	while(1){

		leido=readline("nuevo nombre:");
		config_set_value(confi,"nomre",leido);
		readline("press enter to save");
		config_save(confi);


	}
}

