#include <commons/collections/dictionary.h>
#include <commons/config.h>
#include <readline/readline.h>
#include <stdlib.h>
int main(){

	t_config *config = config_create("nugaton.txt");
	config_save_in_file(config, "nugaton.txt");


	char* leido;

	while(1){

		leido=readline("nuevo nombre:");

		config_set_value(config,"nombre",leido);

		readline("press enter to save");

		config_save_in_file(config, "nugaton.txt");

		config_save(config);


	}

}

